{
  "name": "Luis's Theme",
  "type": "dark",
  "colors": {
    "foreground": "#adb5bd",
    "focusBorder": "#3b82f6",
    "widget.shadow": "#000",
    "button.background": "#1d4ed8",
    "button.foreground": "#dee2e6",
    "button.hoverBackground": "#3b82f6",
    "dropdown.background": "#000",
    "dropdown.foreground": "#dee2e6",
    "dropdown.border": "#3b82f6",
    "input.background": "#000",
    "input.foreground": "#dee2e6",
    "input.placeholderForeground": "#495057",
    "inputOption.activeBorder": "#3b82f6",
    "inputValidation.errorBorder": "#ef4444",
    "inputValidation.errorBackground": "#7f1d1d",
    "inputValidation.infoBorder": "#3b82f6",
    "inputValidation.infoBackground": "#1e3a8a",
    "inputValidation.warningBorder": "#eab308",
    "inputValidation.warningBackground": "#713f12",
    "scrollbar.shadow": "#000",
    "scrollbarSlider.background": "#0b0c0f",
    "scrollbarSlider.activeBackground": "#272C36",
    "scrollbarSlider.hoverBackground": "#15181d",
    "badge.background": "#3b82f6",
    "badge.foreground": "#dee2e6",
    "progressBar.background": "#3b82f6",
    "list.activeSelectionBackground": "#15181d",
    "list.activeSelectionForeground": "#dee2e6",
    "list.hoverBackground": "#15181d",
    "list.hoverForeground": "#dee2e6",
    "list.inactiveSelectionBackground": "#15181d",
    "list.inactiveSelectionForeground": "#dee2e6",
    "list.dropBackground": "#000",
    "activityBar.background": "#0b0c0f",
    "activityBar.dropBackground": "#0b0c0f",
    "activityBar.foreground": "#ced4da",
    "activityBar.inactiveForeground": "#495057",
    "activityBarBadge.background": "#3b82f6",
    "activityBarBadge.foreground": "#ced4da",
    "sideBar.background": "#0b0c0f",
    "sideBar.foreground": "#adb5bd",
    "sideBarTitle.foreground": "#adb5bd",
    "sideBar.border": "#39414F",
    "sideBarSectionHeader.background": "#101114",
    "editorGroupHeader.noTabsBackground": "#0b0c0f",
    "editorGroup.dropBackground": "#0b0c0faa",
    "editorGroupHeader.tabsBackground": "#0b0c0f",
    "tab.activeForeground": "#dee2e6",
    "tab.inactiveForeground": "#495057",
    "tab.border": "#39414F",
    "tab.inactiveBackground": "#0b0c0f",
    "tab.activeBackground": "#0b0c0f",
    "editor.background": "#0b0c0f",
    "editor.foreground": "#adb5bd",
    "editorCursor.foreground": "#adb5bd",
    "editorLineNumber.foreground": "#343a40",
    "editorLineNumber.activeForeground": "#adb5bd",
    "editorLink.activeForeground": "#60a5fa",
    "editorWhitespace.foreground": "#212529",
    "editorIndentGuide.background": "#212529",
    "editorBracketMatch.background": "#272C36",
    "editorOverviewRuler.findMatchForeground": "#5dcdfd55",
    "editorOverviewRuler.modifiedForeground": "#eab308",
    "editorOverviewRuler.addedForeground": "#22c55e",
    "editorOverviewRuler.deletedForeground": "#991b1b",
    "editorOverviewRuler.errorForeground": "#ef4444",
    "editorOverviewRuler.warningForeground": "#854d0e",
    "editorOverviewRuler.infoForeground": "#a855f7",
    "editorGutter.background": "#0b0c0f",
    "editorGutter.modifiedBackground": "#eab308",
    "editorGutter.addedBackground": "#22c55e",
    "editorGutter.deletedBackground": "#ef4444",
    "editorWidget.background": "#0b0c0f",
    "editorWidget.border": "#495057",
    "editorSuggestWidget.foreground": "#dee2e6",
    "peekView.border": "#39414F",
    "peekViewEditor.background": "#0b0c0f",
    "peekViewEditorGutter.background": "#0b0c0f",
    "peekViewResult.background": "#0b0c0f",
    "peekViewTitle.background": "#0b0c0f",
    "peekViewTitleLabel.foreground": "#adb5bd",
    "peekViewTitleDescription.foreground": "#adb5bd",
    "peekViewResult.selectionForeground": "#adb5bd",
    "peekViewResult.fileForeground": "#adb5bd",
    "peekViewResult.lineForeground": "#adb5bd",
    "statusBar.background": "#3b82f6",
    "statusBar.foreground": "#dee2e6",
    "statusBar.noFolderBackground": "#3b82f6",
    "statusBarItem.remoteBackground": "#22c55e",
    "titleBar.activeBackground": "#0b0c0f",
    "titleBar.activeForeground": "#adb5bd",
    "titleBar.inactiveBackground": "#0b0c0f",
    "gitDecoration.modifiedResourceForeground": "#eab308",
    "gitDecoration.deletedResourceForeground": "#ef4444",
    "gitDecoration.untrackedResourceForeground": "#22c55e",
    "gitDecoration.ignoredResourceForeground": "#495057",
    "gitDecoration.conflictingResourceForeground": "#a855f7",
    "terminal.background": "#0b0c0f",
    "terminal.foreground": "#adb5bd",
    "listFilterWidget.noMatchesOutline": "#ef4444",
    "listFilterWidget.outline": "#212529",
    "listFilterWidget.shadow": "#000",
    "list.focusAndSelectionOutline": "#00000000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#495057"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#10B981"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": [
        "string.regexp",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#dcfce7"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Variable, number, Built-in constants, separators like ; or ,",
      "scope": [
        "variable",
        "variable.parameter.function-call",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Member Variable",
      "scope": [
        "variable.member"
      ],
      "settings": {
        "foreground": "#ef4444"
      }
    },
    {
      "name": "Storage, keyewrod",
      "scope": [
        "storage",
        "keyword",
        "keyword.operator",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "JavaScript/TypeScript interpolation punctuation",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Interpolation text",
      "scope": [
        "meta.embedded"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
        "source.haskell storage.type",
        "source.c storage.type"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Inherited class type",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Lambda arrow",
      "scope": [
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Java primitive variable types",
      "scope": [
        "source.java storage.type.primitive"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Function name and arguments",
      "scope": [
        "entity.name.function",
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#ef4444"
      }
    },
    {
      "name": "Imports and packages",
      "scope": [
        "entity.name.import",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Entity name",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "JSX Component",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class",
        "source.go storage.type"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "CSS and sass pseudo elements",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.name.tag.reference.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "CSS tag names",
      "scope": [
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag",
        "punctuation.separator.key-value.scss"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Css regex character",
      "scope": [
        "constant.character.escape.scss"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "scope": [
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "CSS Class and Support - Property Value",
      "scope": [
        "source.css support.constant.property-value",
        "source.sass support.constant.property-value",
        "source.scss support.constant.property-value",
        "source.less support.constant.property-value",
        "source.stylus support.constant.property-value",
        "source.postcss support.constant.property-value"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "CSS Class and Support - Property Name",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "CSS entities",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "CSS Font",
      "scope": [
        "support.constant.font-name",
        "meta.attribute-selector.scss",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#10B981"
      }
    },
    {
      "name": "CSS puntuation",
      "scope": [
        "support.type.property-name.media.css",
        "meta.property-name.media-query.scss",
        "punctuation.definition.attribute-selector.end.bracket.square.scss",
        "punctuation.definition.attribute-selector.begin.bracket.square.scss"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "CSS classes",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.parent-selector-suffix.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#10B981"
      }
    },
    {
      "name": "CSS Name variable",
      "scope": [
        "variable.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D97706"
      }
    },
    {
      "name": "CSS keywords",
      "scope": [
        "keyword.other.unit.rem.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.vh.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Search Results Match Numbers",
      "scope": [
        "constant.numeric.line-number.match"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Search Results Lines",
      "scope": [
        "entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "JSON constants",
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Markup heading",
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Markup links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f87171"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f87171"
      }
    },
    {
      "name": "Markup Bold/italic",
      "scope": [
        "markup.italic markup.bold",
        "markup.bold markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.raw"
      ],
      "settings": {
        "background": "#bfbdb605"
      }
    },
    {
      "name": "Markup Code Inline",
      "scope": [
        "markup.raw.inline"
      ],
      "settings": {
        "background": "#bfbdb60f"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "background": "#bfbdb60f",
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "Markup Blockquote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6ee7b7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup List Bullet",
      "scope": [
        "markup.list punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#ffb454"
      }
    },
    {
      "name": "Markup added",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#7fd962"
      }
    },
    {
      "name": "Markup modified",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#73b8ff"
      }
    },
    {
      "name": "Markup removed",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#f26d78"
      }
    },
    {
      "name": "Markup Strike",
      "scope": [
        "markup.strike"
      ],
      "settings": {
        "foreground": "#e6b673"
      }
    },
    {
      "name": "Markup Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "background": "#bfbdb60f",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw"
      ],
      "settings": {
        "foreground": "#f29668"
      }
    },
    {
      "name": "Markdown - Line Break",
      "scope": [
        "text.html.markdown meta.dummy.line-break"
      ],
      "settings": {
        "background": "#acb6bf8c",
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": [
        "punctuation.definition.markdown"
      ],
      "settings": {
        "background": "#ced4da",
        "foreground": "#acb6bf8c"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "parameter.label": "#ced4da"
  }
}