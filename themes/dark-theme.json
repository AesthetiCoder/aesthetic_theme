    "editorOverviewRuler.deletedForeground": "#991b1b",
    "editorOverviewRuler.errorForeground": "#ef4444",
    "editorOverviewRuler.warningForeground": "#854d0e",
    "editorOverviewRuler.infoForeground": "#a855f7"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#2dd4bf"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": [
        "string.regexp",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#dcfce7"
      }
    },
    {
      "name": "Variable, number, Built-in constants, separators like ; or ,",
      "scope": [
        "variable",
        "variable.parameter.function-call",
        "constant.language",
        "constant.numeric",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#dee2e6"
      }
    },
    {
      "name": "Member Variable",
      "scope": [
        "variable.member"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Storage, keyewrod",
      "scope": [
        "storage",
        "keyword",
        "keyword.operator",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "JavaScript/TypeScript interpolation punctuation",
      "scope": [
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Interpolation text",
      "scope": [
        "meta.embedded"
      ],
      "settings": {
        "foreground": "#dee2e6"
      }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
        "source.haskell storage.type",
        "source.c storage.type"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Inherited class type",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Lambda arrow",
      "scope": [
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Java primitive variable types",
      "scope": [
        "source.java storage.type.primitive"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Function name and arguments",
      "scope": [
        "entity.name.function",
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#dee2e6"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Imports and packages",
      "scope": [
        "entity.name.import",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Entity name",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "JSX Component",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#3b82f680"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class",
        "source.go storage.type"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#d95757"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#c594c5"
      }
    },
    {
      "name": "CSS tag names",
      "scope": [
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag"
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "CSS browser prefix",
      "scope": [
        "source.css support.type",
        "source.sass support.type",
        "source.scss support.type",
        "source.less support.type",
        "source.stylus support.type"
      ],
      "settings": {
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "CSS Class and Support - Property Value",
      "scope": [
        "source.css support.constant.property-value",
        "source.sass support.constant.property-value",
        "source.scss support.constant.property-value",
        "source.less support.constant.property-value",
        "source.stylus support.constant.property-value",
        "source.postcss support.constant.property-value"
      ],
      "settings": {
        "foreground": "#dee2e6"
      }
    },
    {
      "name": "CSS Class and Support - Property Name",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "CSS entities",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "CSS Font",
      "scope": [
        "support.constant.font-name",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#2dd4bf"
      }
    },
    {
      "name": "CSS puntuation",
      "scope": [
        "punctuation.separator.key-value.scss"
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "CSS classes",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.parent-selector-suffix.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4ade80"
      }
    },
    {
      "name": "CSS Name variable",
      "scope": [
        "variable.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fb923c"
      }
    },
    {
      "name": "CSS pseudo elements",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#38bdf8"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c084fc"
      }
    },
    {
      "name": "CSS keywords",
      "scope": [
        "keyword.other.unit.rem.css",
        "keyword.other.unit.s.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Search Results Numbers",
      "scope": [
        "constant.numeric.line-number.find-in-files - match"
      ],
      "settings": {
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "Search Results Match Numbers",
      "scope": [
        "constant.numeric.line-number.match"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Search Results Lines",
      "scope": [
        "entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#22c55e"
      }
    },
    {
      "scope": [
        "message.error"
      ],
      "settings": {
        "foreground": "#d95757"
      }
    },
    {
      "name": "JSON constants",
      "scope": [
        "constant.language.json"
      ],
      "settings": {
        "foreground": "#38bdf8"
      }
    },
    {
      "name": "Markup heading",
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#22c55e"
      }
    },
    {
      "name": "Markup links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f87171"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f87171"
      }
    },
    {
      "name": "Markup Bold/italic",
      "scope": [
        "markup.italic markup.bold",
        "markup.bold markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.raw"
      ],
      "settings": {
        "background": "#bfbdb605"
      }
    },
    {
      "name": "Markup Code Inline",
      "scope": [
        "markup.raw.inline"
      ],
      "settings": {
        "background": "#bfbdb60f"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "background": "#bfbdb60f",
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "Markup Blockquote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6ee7b7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup List Bullet",
      "scope": [
        "markup.list punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#ffb454"
      }
    },
    {
      "name": "Markup added",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#7fd962"
      }
    },
    {
      "name": "Markup modified",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#73b8ff"
      }
    },
    {
      "name": "Markup removed",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#f26d78"
      }
    },
    {
      "name": "Markup Strike",
      "scope": [
        "markup.strike"
      ],
      "settings": {
        "foreground": "#e6b673"
      }
    },
    {
      "name": "Markup Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "background": "#bfbdb60f",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw"
      ],
      "settings": {
        "foreground": "#f29668"
      }
    },
    {
      "name": "Markdown - Line Break",
      "scope": [
        "text.html.markdown meta.dummy.line-break"
      ],
      "settings": {
        "background": "#acb6bf8c",
        "foreground": "#acb6bf8c"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": [
        "punctuation.definition.markdown"
      ],
      "settings": {
        "background": "#dee2e6",
        "foreground": "#acb6bf8c"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "parameter.label": "#dee2e6"
  }
}