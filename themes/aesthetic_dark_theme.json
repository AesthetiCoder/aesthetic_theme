  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#495057"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#059669"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": [
        "string.regexp",
        "source.regexp",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#A7F3D0"
      }
    },
    {
      "name": "Constant color",
      "scope": [
        "constant",
        "constant.language",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Variable, number, Built-in constants, separators like ; or ,",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.parameter.function-call",
        "variable.other",
        "variable.language",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Function name and arguments",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#1e40af"
      }
    },
    {
      "name": "Storage, keyword",
      "scope": [
        "storage",
        "storage.type",
        "keyword",
        "keyword.operator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class",
        "support.function"
      ],
      "settings": {
        "foreground": "#0891B2"
      }
    },
    {
      "name": "CSS Class and Support - Property Name",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "CSS Class and Support - Property Value",
      "scope": [
        "constant.numeric.css",
        "source.css support.constant.property-value",
        "source.sass support.constant.property-value",
        "source.scss support.constant.property-value",
        "source.less support.constant.property-value",
        "source.stylus support.constant.property-value",
        "source.postcss support.constant.property-value"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "CSS tag names",
      "scope": [
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag",
        "punctuation.separator.key-value.scss"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "CSS and sass pseudo elements",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.name.tag.reference.scss"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Css regex character",
      "scope": "constant.character.escape.scss",
      "settings": {
        "foreground": "#A7F3D0"
      }
    },
    {
      "name": "CSS constants and entities",
      "scope": [
        "support.constant.property-value.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#0891B2"
      }
    },
    {
      "name": "CSS Font",
      "scope": [
        "support.constant.font-name",
        "meta.attribute-selector.scss",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#059669"
      }
    },
    {
      "name": "CSS punctuation",
      "scope": [
        "support.type.property-name.media.css",
        "meta.property-name.media-query.scss",
        "punctuation.definition.attribute-selector.end.bracket.square.scss",
        "punctuation.definition.attribute-selector.begin.bracket.square.scss",
        "punctuation.definition.media-query.begin.bracket.round.scss",
        "punctuation.definition.media-query.end.bracket.round.scss"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "CSS classes",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.parent-selector-suffix.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#059669"
      }
    },
    {
      "name": "CSS Name variable",
      "scope": "variable.scss",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D97706"
      }
    },
    {
      "name": "CSS keywords",
      "scope": [
        "keyword.other.unit.rem.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.s.css",
        "keyword.other.unit.vh.css"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Css if and media",
      "scope": [
        "meta.at-rule.if.scss",
        "support.constant.media.css"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "JSON constants",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#0891B2"
      }
    },
    {
      "name": "JSON name",
      "scope": "support.type.property-name.json",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Colors of the object",
      "scope": [
        "meta.object-literal.key.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0284C7"
      }
    },
    {
      "name": "TS types",
      "scope": [
        "support.type.primitive.ts"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0891B2"
      }
    },
    {
      "name": "Template string",
      "scope": [
        "meta.template.expression.ts"
      ],
      "settings": {
        "foreground": "#ced4da"
      }
    },
    {
      "name": "Markdown paragraph",
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#adb5bd"
      }
    },
    {
      "name": "Markdown Titles",
      "scope": [
        "entity.name.section.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0EA5E9"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markdown strikethrough",
      "scope": "markup.strikethrough.markdown",
      "settings": {
        "foreground": "#ca8a04"
      }
    },
    {
      "name": "Markdown string link",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#10B981"
      }
    },
    {
      "name": "Markdown link",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markdown reference constant",
      "scope": "constant.other.reference.link.markdown",
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Markdown code",
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markdown code definition",
      "scope": [
        "fenced_code.block.language.markdown",
        "punctuation.definition.raw.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "Markdown List",
      "scope": [
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markdown quote",
      "scope": [
        "markup.quote.markdown"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a855f7"
      }
    }
  ],
  "semanticHighlighting": true
}