{
  "name": "luis-theme",
  "displayName": "Luis's Theme",
  "description": "My personal color schema themes for vscode",
  "version": "0.0.1",
  "categories": [
    "Themes"
  ],
  "engines": {
    "vscode": "^1.74.0",
    "node": "^14.18.0 || >=16.0.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=7.14.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint --ext .ts . --fix",
    "dev": "nodemon",
    "build": "rimraf dist && tsc",
    "package": "rimraf luis-theme-0.0.1.vsix && vsce package --no-dependencies"
  },
  "contributes": {
    "iconThemes": [
      {
        "id": "dark",
        "label": "Dark Icons",
        "path": "./themes/dark-icon-theme.json"
      }
    ],
    "themes": [
      {
        "label": "Luis's Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vscode/vsce": "^2.18.0",
    "eslint": "^8.37.0",
    "eslint-config-typescript-coder-dev": "^0.0.2",
    "nodemon": "^2.0.20",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}